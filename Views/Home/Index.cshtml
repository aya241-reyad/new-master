@{
    ViewData["Title"] = "الرئيسيه";
}

@model IEnumerable<Miar.Models.RealState>


@section style {


}


<header class="masthead text-white" id="FirstSec">
    <div class="masthead-content">
        <div class="container px-5">

            <h1 class="masthead-heading mb-0">لؤلؤه العين</h1>
            <h2 id="slogin" class="masthead-subheading mb-0">اختيارك لنا يحافظ علي اثاثك</h2>
            <h4 id="slogin" class="masthead-subheading slgon3" style="font-size:1rem;">
                نعمل وفق منهجية واضحة ومتطورة لنقل الاثاث

            </h4>
            <h4 id="slogin" class="masthead-subheading slgon2" style=" font-size: 0.83rem;">
                نوجد أفضل عربات نقل الاثاث للحصول علي افضل خدمه .
            </h4>

        </div>
    </div>

</header>

<!-- Content section 2-->
<section class="sectionContent" id="HowAre">
    <div class="container px-5" id="whoAreSection">
        <div class="row">
            <div class="col-md-6">
                <div class="p-5"><img class="img-fluid rounded-circle" src="~/img/furniture.jpg" alt="..." /></div>
            </div>
            <div class="col-md-6">
                <div class="p-5">
                    <h2 class="display-4">رؤيتنا </h2>
                    <p>
                        لدينا افضل الخدمات التي نقدمها في شركتنا نقوم بفك وتركيب الاثاث من خلال افضل النجاريين والفنيين الذين يشرفون علي هذه المهمه،
                    </p>
                    <p>
                        نقوم بتقديم افضل خدمه لكم اتصل بنا الان ،
                    </p>
                    <p>
                     سيارات نقل العفش تقوم شركتنا  بالتطوير من الخدمات دائما للحصول علي تقديم افضل خدمه .
                    </p>
                </div>
            </div>
        </div>
    </div>
</section>



<section class="sectionContent" id="SectionContact">
    <div class="container px-5">
        <h2 id="HeaderFro3" class="text-center display-4">خدمتنا</h2>
        <div class="row" id="LocContent">
                <div class="col-lg-4 State">
                    <img src="~/img/004.jpg" />
                    <div>
                        <h4 class="HeaderLoc">فك وتركيب الأثاث</h4>
                        <p>
                           فك وتركيب الأثاث من خلال عمال ونجارين متخصصين في فك وتركيب الأثاث والستائروالأجهزةالمنزلية والتكيفات .
                        </p>
                    </div>
                    </div>
                 <div class="col-lg-4 State">
                    <img src="~/img/006.jpg" />
                    <div>
                        <h4 class="HeaderLoc">تغليف الأثاث</h4>
                        <p>
                           تغليف الأثاث أهم خطوة من خطوات عملية نقل الأثاث وذلك للحفاظ علي اساس منزلك من الخدوش والتجريح والأتربة .
                        </p>
                    </div>
                    </div>
                 <div class="col-lg-4 State">
                    <img src="~/img/005.jpg" />
                    <div>
                        <h4 class="HeaderLoc">ونش رفع أثاث</h4>
                        <p>
                          متوفر لدينا أوناش لرفع وتنزيل الأثاث من وإلي أعلي المرتفعات والأماكن الضيقة لضمان وصوله بأمان تام .
                        </p>
                    </div>
                    </div>
            





        </div>


    </div>


</section>


<section class="sectionContent" id="sectionContentEmail">
    <div class="container px-5">
        <h2 id="HeaderFro3" class="text-center display-4">تواصل معنا</h2>
        <div class="row" id="LocContent">

            <div class="col-lg-6">
                <div hidden class="alert   fade show alert-success" role="alert">
                    تم ارسال الرساله بنجاح , سيتم التواصل معكم من خلال الهاتف شكرا
                </div>
                <form id="MessageForm" method="post">
                    <div class="form-group">
                        <label class="control-label text-white"> الاسم *</label>
                        <input name="Name" id="Name" class="form-control" />
                        <span hidden id="NameDanger" class="text-danger">ادخل هذا الحقل</span>
                    </div>
                    <div class="form-group">
                        <label class="control-label text-white">رقم الهاتف *</label>
                        <input hi name="Tele" id="Phone" class="form-control" />
                        <span hidden id="PhoneDanger" class="text-danger">ادخل هذا الحقل</span>
                    </div>
                    <div class="form-group">
                        <label class="control-label text-white">الرساله *</label>
                        <textarea name="Message" id="Message" class="form-control"></textarea>
                        <span hidden id="MessageDanger" class="text-danger">ادخل هذا الحقل</span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="إرسال" class="btn btn-primary" style="width:100%" />
                    </div>
                </form>

            </div>

            <div class="col-lg-6" style="padding-top:10px">
                <div id="" style="width:100%;height:250px">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m24!1m12!1m3!1d14084.573844926725!2d31.162204187477936!3d30.973178793381738!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!4m9!3e6!4m3!3m2!1d30.9763086!2d31.159583599999998!4m3!3m2!1d30.96471!2d31.162347399999998!5e0!3m2!1sar!2seg!4v1631129842064!5m2!1sar!2seg" style=" width:100%;height:90%; border:0;" allowfullscreen="" loading="lazy"></iframe>
                </div>
            </div>



        </div>


    </div>


</section>
@section Scripts {
    <script src="~/js/MessageIndex.js"></script>

}
